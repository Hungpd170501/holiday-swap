{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "rabbitmq",
      "host": {
        "sourcePath": "/var/lib/rabbitmq"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "holiday-swap",
      "image": "hungpd170501/holiday-swap:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "dev"
        }
      ]
    },
    {
      "name": "rabbitmq",
      "image": "rabbitmq:3-management-alpine",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 5672,
          "containerPort": 5672
        },
        {
          "hostPort": 15672,
          "containerPort": 15672
        }
      ]
    }
  ]
}
