{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "rabbitmq",
      "host": {
        "sourcePath": "/var/lib/rabbitmq"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "holiday-swap",
      "image": "hungpd170501/holiday-swap:29.11.2023.18.23.45",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "RABBITMQ_HOST",
          "value": "rabbitmq"
        }
      ],
      "links": [
        "rabbitmq",
        "redis"
      ]
    },
    {
      "name": "rabbitmq",
      "image": "rabbitmq:3-management-alpine",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 5672,
          "containerPort": 5672
        },
        {
          "hostPort": 15672,
          "containerPort": 15672
        },
        {
          "hostPort": 61613,
          "containerPort": 61613
        }
      ]
    },
    {
      "name": "redis",
      "image": "redis:alpine",
      "essential": false,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 6379,
          "containerPort": 6379
        }
      ]
    }
  ]
}
